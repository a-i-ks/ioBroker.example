{
  "name": "iobroker.template",
  "version": "0.0.1",
  "description": "Template for adapter development",
  "author": {
    "name": "Author",
    "email": "author@mail.com"
  },
  "homepage": "https://github.com/Author/ioBroker.template",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "template",
    "Smart Home",
    "home automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Author/ioBroker.template"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.5.1"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^2.2.2",
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@iobroker/adapter-react": "2.0.13",
    "@iobroker/testing": "^2.5.2",
    "@material-ui/core": "^4.12.3",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gulp": "^4.0.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.5",
    "@types/proxyquire": "^1.3.28",
    "@types/react-dom": "^16.9.14",
    "@types/react": "^16.14.21",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.8",
    "axios": "^0.24.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint": "^7.32.0",
    "gulp": "^4.0.2",
    "mocha": "^9.1.3",
    "parcel-bundler": "^1.12.5",
    "proxyquire": "^2.1.3",
    "react-dom": "^16.14.0",
    "react": "^16.14.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "typescript": "~4.4.4"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "watch:parcel": "parcel admin/src/index.jsx admin/src/tab.tsx -d admin/build",
    "build:parcel": "parcel build admin/src/index.jsx admin/src/tab.tsx -d admin/build",
    "build": "npm run build:parcel",
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint --ext .js,.jsx",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/Author/ioBroker.template/issues"
  },
  "readmeFilename": "README.md"
}